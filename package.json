{
  "name": "@rahlir/theme-gruvbox",
  "version": "1.1.0",
  "description": "Gruvbox dark theme for Jupyter Lab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyterlab-theme",
    "gruvbox",
    "gruvbox-theme"
  ],
  "homepage": "https://github.com/Rahlir/theme-gruvbox",
  "bugs": {
    "url": "https://github.com/Rahlir/theme-gruvbox/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Tadeas Uhlir",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rahlir/theme-gruvbox.git"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typescript": "~3.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themeDir": "style",
    "themePath": "style/index.css"
  }
}
